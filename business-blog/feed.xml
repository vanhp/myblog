<?xml version="1.0" encoding="utf-8"?>
<feed xmlns="http://www.w3.org/2005/Atom">
<!-- <generator uri="https://jekyllrb.com/" version="4.1.1">Jekyll</generator> -->


<link href="{{- site.url -}}{{- site.baseurl -}}/business-blog/feed.xml" rel="self" type="application/atom+xml"/>
<link href="{{- site.url -}}{{- site.baseurl -}}/business-blog/" rel="alternate" type="text/html"/>
<updated>{{- site.time | date_to_xmlschema -}}</updated>
<id>{{- site.url -}}{{- site.baseurl -}}/business-blog</id>
<title> {{- site.title -}} _ Business Blog</title> 
<subtitle>{{- site.description | strip_html | lstrip | rstrip  -}}</subtitle>
 <author>
    <name>{%- if site.author -%}
            {{- site.author -}}
        {%- else -%}
            {{- site.title -}}
            {%- endif -%}</name>
</author>
{%- for post in site.posts limit: 20 -%}
    {%- if post.category == "Business" -%}
        <entry>
            <title> {{- post.title -}}</title> 
            <id>{{- site.url -}}{{- site.baseurl -}}{{- post.url -}}</id>
            <link href="{{- site.url -}}{{- site.baseurl -}}{{- post.url -}}" />
            <updated>{{- post.date | date_to_xmlschema -}} </updated>
            <summary{%- if post.excerpt -%}
                    {{- post.excerpt | strip_html | lstrip | rstrip  -}}
                {%- endif -%} />
        </entry>
    {%- endif -%}
{%- endfor -%}

